---
import Proyecto from './Proyecto.astro'
const proyectos = ['Proyecto1', 'Proyecto2']
---

<main class="flex flex-col gap-3 p-6 text-white">
  <h2 class='text-4xl font-semibold text-center text-green-500'>&lt; Proyectos /&gt;</h2>

  {
    proyectos.map((proyecto) => {
      return <Proyecto key={proyecto} nombre={proyecto} />
    })
  }
</main>
